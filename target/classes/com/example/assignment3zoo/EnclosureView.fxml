<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="335.0" prefWidth="325.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/25" fx:controller="com.example.assignment3zoo.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <HBox prefHeight="22.0" prefWidth="286.0">
      <children>
         <Button alignment="TOP_LEFT" mnemonicParsing="false" text="Back" />
      </children>
   </HBox>
   <TreeView prefHeight="200.0" prefWidth="200.0" />
   <HBox alignment="CENTER" prefHeight="55.0" prefWidth="286.0" spacing="20.0">
      <children>
          <Button onAction="#onHelloButtonClick" style="-fx-background-color: blue;" text="Add" textFill="WHITE">
            <HBox.margin>
               <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </HBox.margin>
            <font>
               <Font size="14.0" />
            </font>
         </Button>
         <Button mnemonicParsing="false" style="-fx-background-color: blue;" text="Edit" textFill="WHITE">
            <HBox.margin>
               <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </HBox.margin>
            <font>
               <Font size="15.0" />
            </font>
         </Button>
         <Button mnemonicParsing="false" style="-fx-background-color: blue;" text="Delete" textFill="WHITE">
            <HBox.margin>
               <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </HBox.margin>
            <font>
               <Font size="14.0" />
            </font>
         </Button>
      </children>
   </HBox>
</VBox>
